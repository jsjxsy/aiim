	<!--导入struts-default.xml文件 -->	
	<include file="struts-default.xml"></include>
	
	<package name="interceptor" extends="struts-default">
	   <interceptors>
            <!-- 定义权限控制拦截器 -->
            <interceptor name="auth" class="com.orifound.aiim.web.interceptors.AuthInterceptor"/>
            <!-- 定义一个包含权限控制的拦截器栈 -->
			<interceptor-stack name="authdefault">
				<interceptor-ref name="defaultStack"></interceptor-ref>
				<interceptor-ref name="auth"></interceptor-ref>
			</interceptor-stack>
		</interceptors>
		<default-interceptor-ref name="authdefault"></default-interceptor-ref> 
	</package>
	
	<!-- user -->
	<package name="login" extends="struts-default" namespace="/login">
		<action name="userLoginAction_*" class="userLoginAction" method="{1}">
			<result type="redirect">/main.jsp</result>
			<result name="toLogin">/login.jsp</result>
			<result name="out" type="redirect">/login.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		
		<!-- 动态生成验证码 -->
		<action name="getAuthcode" class="getAuthcode">
	        <result type="stream">
	            <param name="contentType">image/jpeg</param>
	            <param name="inputName">inputStream</param>
	        </result>
	    </action>
	</package>
	

	<!-- 移交管理 -->
	<package name="YJGL" extends="struts-default" namespace="/YJGL">
	     <action name="YJJSAction_*" class="YJJSAction" method="{1}">
		      <result>${resultURL}</result>
		 </action>
		  
	      <!-- 登记成功后点继续登记是所用的Action -->
		  <action name="JSDJRefreshAction">
		    <result>/YJJSGL/dlg_JSDJ.jsp</result>
		  </action>
	</package>
	
	<!-- 综合查询 -->
